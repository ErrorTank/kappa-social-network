.dating-matched {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 12px 20px;
    flex-wrap: wrap;

    .img-matched {
        border-radius: 5px;
        flex: 0 0 32%;
        height: 120px;
        position: relative;
        overflow: hidden;
        transition: all 0.2s ease;
        cursor: pointer;
        margin-bottom: 8px;

        img {
            width: 100%;
            height: 100%;
            object-fit: cover;
        }

        .username {
            position: absolute;
            bottom: 5px;
            left: 5px;
            font-size: 15px;
            font-weight: 1000;
            color: white;
        }

        &:hover {
            transform: scale(1.1);
        }
    }

    .h2 {
    }
}