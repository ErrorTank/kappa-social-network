.register-dating-form{
    .wrap{
        margin-bottom: 20px
        margin-top: 50px
        .title{
            font-size: 1.9rem
            font-weight: 500
            margin-bottom :20px
            justify-content: center
            align-items: center
            display: flex
            flex-direction: row
            .dash{
                background-color: common-border
                height : 1px
                width : 200px
            }
            .text-title{
                margin : 0 30px
                font-style: italic
            }
        }
        .row-wrapper{
            display: flex;
            align-items: center
            justify-content: space-between
            .dr-input{
                flex 0 0 30%
                padding : 0
            }
            .age-layout {
                flex 0 0 30%
                height : 56px
                border: 1px solid lightgray 
                border-radius: 6px;
                padding: 26px 16px 10px;
                position relative
                background-color: white
                .label{
                    position absolute
                    transition-property: transform;
                    transition-duration: 200ms;
                    transform: scale(.75) translateY(-11px);
                    transform-origin: top left;  
                    color: gray;
                    font-weight: 400;
                    left: 16px;
                    font-size: 16px;
                    white-space: nowrap;
                    line-height: 20px;
                    top: 18px;
                    max-width 100%;
                    right: 8px;
                }
                .value{
                    font-size: 16px;
                    padding-left: 5px
                }
            }
        }  
    }
    .picture-input-wrapper{
        width 320px
        display: flex
        justify-content: center
        margin: 0 auto
    .picture-input {
    padding: 0 16px;

    .picture-input-header {
      font-weight: 600;
      line-height: 16px;
      font-size: 13px;

      .sub {
        font-weight: normal;
      }
    }

    // display picture
    .images-display {
      margin-top: 8px;

      .images-container {
        display: flex;
        justify-content: flex-start;
        flex-wrap: wrap;
        align-content: flex-start;

        .file-display {
          max-width: 94px;
          max-height: 94px;
          box-sizing: border-box;
          padding: 4px;
          position: static;
          border-radius: 4px;
          height: 94px;
          width: 94px;
          background-color: white;
          margin-right: 0;
          position: relative;

          .file-info {
            width: 100%;
            height: 100%;

            .img-wrapper {
              width: 86px;
              height: 86px;

              img {
                height: 86px;
                width: 86px;
                max-width: 100%;
                max-height: 100%;
                object-fit: cover;
                cursor: pointer;
                border-radius: 4px;
              }
            }
          }
        }

        .add-file {
          cursor: pointer;
          margin: 4px;
          width: 86px;
          height: 86px;
          background-color: grayBackgroundLight;
          border-radius: 4px;
          box-sizing: border-box;
          display: flex;
          flex-direction: column;
          align-items: center;
          justify-content: center;

          i {
            font-size: 16px;
          }

          span {
            font-size: 13px;
            font-weight: 400;
            margin: 2px;
          }
        }
      }
    }

    // empty
    .add-picture-section {
      margin-top: 8px;
      cursor: pointer;
      height: 160px;
      display: flex;
      justify-content: center;
      align-items: center;
      border-radius: 4px;
      border: 1px solid lightgray;

      &.invalid {
        border-color: #d9534f;
      }

      .add-picture-button {
        cursor: pointer;
        border-radius: 6px;
        padding: 0 12px;
        background-color: secondaryBtnLight;
        height: 36px;
        display: flex;
        align-items: center;
        color: normalTextLight;

        i {
          margin: 0 3px;
          font-size: 16px;
        }

        span {
          font-size: 15px;
          font-weight: 600;
          margin: 0 3px;
        }

        &:hover {
          background-color: darken(secondaryBtnLight, 10%);
        }
      }
    }
  }
}
    .button{
            
            font-size : 16px
            font-weight: 500
            color:white
            background-color: blueTextLight
            height :40px
            width : 100px;
            border-radius: 10px;
            margin : 40px auto;
            cursor pointer
            font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif
            display: flex;
            justify-content: center
            align-items: center
            


        }
    .text-center{
        display: flex
        width: 100%
        justify-content: center
        .img-list{
            display : inline-flex
            align-items: center
            width : 315px
            flex-wrap: wrap
        }
    }
}
