.dating-setting{
    position relative
    flex: 0 0 30%;
    background-color: bgLight;
    border-right: 1px solid common-border;
    overflow-y : auto
    .dating-header{
        >i{
            align-self : center
            font-size: 18px;
            font-weight: 500
            color : white
            cursor pointer
        }
    }
    .ds-fitler{
        font-size : 20px
        font-weight: 300    
    }
    .ds-edit{
        // position: absolute;
        .ds-text{
            font-size: 16px;
            font-weight: 500
            margin-bottom: 10px
            display:table;
            margin-left: auto;
            margin-right: auto;
        }
        .ds-wrap{
            .ds-title{
                font-size: 14px;
                font-weight: 400

            }
            .row-wrapper-age{
                display flex;
                .dr-input{
                    flex 0 0 50%
                    padding : 5px 16px
            }

            }
        }
        .dcpf-button{
        margin : 10px auto;
        display: flex
        justify-content: center
        bottom : 0;
        left : 40%
        font-size : 16px
        font-weight: 500
        color:white
        background-color: blueTextLight
        height :30px;
        width : 50px
        border-radius: 10px;
        cursor pointer
        font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif
    }
    }
    
}