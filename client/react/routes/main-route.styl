.app{
  position relative
  width 100vw
  min-height 100vh

  .notification-prompt{
    position fixed
    width 100%
    z-index 2
    top 0
    padding 8px 0
    text-align center
    background-color primaryButton
    color white
    font-weight 500
    font-size 1.5rem

    span{
      text-decoration underline
      cursor pointer
      &:hover{
        color darken(white, 10%)
      }
    }
  }
  #main-route{
    position relative


  }
}