.blur-img-panel{
  width 100%
  height 100%
  overflow hidden
  .img-wrapper{
    width 100%
    height 100%
    filter: blur(8px);
    position relative
    -webkit-filter blur(8px);
    & > img{
      width 100%
      height 100%
      object-fit cover
      position absolute
      absoluteMiddle()
    }
  }


}