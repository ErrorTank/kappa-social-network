.api-schedule-board{
  position relative
  .empty-notify{
    position absolute
    width 100%
    height 100%
    left 0
    top 0
    z-index 2
    display flex
    justify-content center
    align-items center
    background-color rgba(inverse, 0.8)
    color white
    font-size 1.6rem
    font-weight 450
  }
  .loading-inline{
    z-index 3 !important
    i{
      font-size 50px !important
      color inverse !important
    }
  }
}